<!DOCTYPE html>
<html>il
  <head>
    <title>Trails</title>
    <%= csrf_meta_tags %>
    <link href="https://fonts.googleapis.com/css?family=Lato" rel="stylesheet">
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAtMMw_aPV7RuttKlhYXqASAH2euijXXMo&libraries=places"></script>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= cloudinary_js_config %>

    <script src="//widget.cloudinary.com/global/all.js" type="text/javascript"></script>
    <script type="text/javascript">
      window.CLOUDINARY_OPTIONS = {
        cloud_name: "<%= ENV['CLOUD_NAME'] %>",
        upload_preset: "<%= ENV['UPLOAD_PRESET'] %>",
        theme: "minimal",
        image_metatdata: 'true',
        keep_iptc: 'true'
      };
      // window.CLOUDINARY_UPLOAD_PRESET = "<%= ENV['UPLOAD_PRESET'] %>" ;
    </script>

  </head>

  <body>
    <%= yield %>
    <%= render 'layouts/footer' %>
  </body>
</html>
